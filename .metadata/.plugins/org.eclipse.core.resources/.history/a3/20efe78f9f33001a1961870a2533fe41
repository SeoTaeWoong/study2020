package swing;

import java.awt.Canvas;
import java.awt.Graphics;

public class DrCanvas extends Canvas{

	int x1,x2,y1,y2;
	boolean fill;
	String color, figure;
	
	public DrCanvas(String x1, String x2, String y1, String y2, boolean fill, String color, String figure){
		this.x1 = Integer.parseInt(x1);
		this.x2 = Integer.parseInt(x2);
		this.y1 = Integer.parseInt(y1);
		this.y2 = Integer.parseInt(y2);
		this.fill = fill;
		this.color=color;
		this.figure=figure;
	}
	
	@Override
	public void paint(Graphics g) {
		
	}
}
