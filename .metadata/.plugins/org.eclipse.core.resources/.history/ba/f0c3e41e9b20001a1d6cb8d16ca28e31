package instance;

public class Practice {
	public static void main(String[] args) {
		
		int a = 50;
		double b = 178.5;
		char c = 'a';
		boolean d = true;
		String e = "홍길동";
	
		System.out.println("문제1. 정수50, 실수178.5, 문자'A', boolean 값 true\n           문자열\"홍길동\"을 변수에 저장하고 출력하세요.");
		System.out.println("정수 : "+a+"\n실수 : "+b+"\n문자 : "+c+"\nboolean : "+d+"\n문자 : "+e);
	
	}
}
