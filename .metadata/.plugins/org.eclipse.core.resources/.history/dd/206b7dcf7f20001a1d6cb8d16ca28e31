package question;

public class Result {
	public static void main(String[] args) {
		question1();
	}
	
	public static void question1() {
		String a = "100";
		String b = "200";
		
		System.out.println("문제1. \nString형 a와 b의 값을 형변환해서 두수의 곱을 구하시오.");
		System.out.println("답: "+ Integer.parseInt(a)+Integer.parseInt(b));
	}
}
