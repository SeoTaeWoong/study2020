package instance;

public class NumberTest {
	public static void main(String[] args) {
		short a = 320;
		short b = 258;
		int sum = a + b;
		int sub = a - b;
		int mul = a * b;
		double div = a / b;
		
		System.out.println(a+ " + " + b + " = " + sum);
		System.out.println(a+ " - " + b + " = " + sum);
		System.out.println(a+ " * " + b + " = " + sum);
		System.out.println(a+ " / " + b + " = " + sum);
	}
}
