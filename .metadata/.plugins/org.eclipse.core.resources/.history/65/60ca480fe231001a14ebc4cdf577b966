package thread;

class JoinTest implements Runnable{

	@Override
	public void run() {
		// TODO Auto-generated method stub
		for(int i = 1; i<=5; i++) {
			System.out.println(i);
			
			try {
				Thread.sleep(500);
			}catch(InterruptedException e) {
				e.getStackTrace();
			}
		}
	}
	
}
//----
public class JoinMain {

}
