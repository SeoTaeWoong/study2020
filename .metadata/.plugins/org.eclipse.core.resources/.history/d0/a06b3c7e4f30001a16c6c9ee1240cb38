import java.awt.event.ActionListener;


public class Card {
	private Frame frame;
	
	public Card() {
		frame = new Frame();
		
		frame.setTitle("¿¬½À");
		frame.setBounds(700,300,300,300);
		frame.setVisible(true);
		
		frame.addActionListener(new ActionListener() {
			@Override
