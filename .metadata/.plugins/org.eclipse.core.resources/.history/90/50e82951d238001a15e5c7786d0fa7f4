package network;

import java.io.IOException;
import java.net.ServerSocket;

public class ChatServer {
	ServerSocket serverSocket;
	public ChatServer() {
		try {
			while() {
				serverSocket = new ServerSocket(9500);
				System.out.println("서버 준비 완료");
			}
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
	public static void main(String[] args) {
		new ChatServer();
	}
}
