package collection;

import java.text.DecimalFormat;
import java.text.Format;

public class SungJuckDTO {
	
	private int key;
	private String name;
	private int kor;
	private int eng;
	private int math;
	private int total;
	private double avg;
	
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getKey() {
		return key;
	}

	public void setKey(int key) {
		this.key = key;
	}

	public int getKor() {
		return kor;
	}

	public void setKor(int kor) {
		this.kor = kor;
	}

	public int getEng() {
		return eng;
	}

	public void setEng(int eng) {
		this.eng = eng;
	}

	public int getMath() {
		return math;
	}

	public void setMath(int meth) {
		this.math = meth;
	}
	public void setTotal(int total) {
		this.total = total;
	}
	public int getTotal() {
		return total;
	}
	public void setArg(double avg) {
		this.avg = avg;
	}
	
	public String toString() {
		
		return key+"\t"+name+"\t"+kor+"\t"+eng+"\t"+math+"\t"+total+"\t"+new DecimalFormat("#.##").format(avg);
	}
	
}
