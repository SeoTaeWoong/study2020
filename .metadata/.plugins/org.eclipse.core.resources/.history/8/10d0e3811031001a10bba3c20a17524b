package collection;

public class PersonDTO {
	private String name;
	private int age;
	
	public PersonDTO() {
		
	}
	
	public PersonDTO(String name, int age) {
		this.name=name;
		this.age=age;
	}
	
	public void setName(String name) {
		this.name=name;
	}
	public String getName() {
		return name;
	}
	
	public void setAge() {
		this.age=age;
	}
	public String getAge() {
		return age;
	}
	
	
}
