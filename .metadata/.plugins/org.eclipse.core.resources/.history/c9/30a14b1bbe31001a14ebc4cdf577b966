public class Test07 {
	public static void main(String[] args) {
		DecimalFormat df = new DecimalFormat();
		StringBuffer a = new StringBuffer();
		int index = 0;
		a.append("0123.023");
		if(a.indexOf(".")!=-1) {
			System.out.println(a.indexOf("."));
		}
		;
		System.out.println(df.format(Double.parseDouble(a.toString())));
		
	}
}
